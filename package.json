{
  "name": "@daxplrer/json_simplified",
  "version": "1.0.4",
  "description": "just a simplified json",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "./run": {
      "require": "./lib/cjs/quick.js",
      "import": "./lib/esm/quick.js",
      "default": "./lib/cjs/quick.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jest.config.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daxplrer/json_simplified.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "daxplrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daxplrer/json_simplified/issues"
  },
  "homepage": "https://github.com/daxplrer/json_simplified#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/semver": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "js-sdsl": "^4.4.2",
    "semver": "^7.5.4",
    "ts-jest": "^29.0.5",
    "typescript": "5.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "path-browserify": "^1.0.1"
  }
}
