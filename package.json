{
  "name": "@daxplrer/alkitabapi",
  "version": "1.0.2",
  "description": "alkitab api, as a module (ts types included)",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "exports": {
    "types" :{
      "import": "./lib/esm/index.d.ts",
      "require": "./lib/cjs/index.d.ts",
      "default": "./lib/esm/index.d.ts"
    }
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "node copycjstoesm.cjs && tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daxplrer/alkitab-api.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "daxplrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daxplrer/alkitab-api/issues"
  },
  "homepage": "https://github.com/daxplrer/alkitab-api#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/semver": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.4.1",
    "js-sdsl": "^4.4.2",
    "semver": "^7.5.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12"
  }
}
